{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pfe": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/pfe",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/assets/css/base.css",
              "src/assets/css/theme-default.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/animate.min.css",
              "src/assets/css/settings.css",
              "src/assets/css/style.css",
              "src/assets/css/media_queries.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/shotcodes.css",
              "src/assets/css/jquery.fancybox.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/owl.theme.css",
              "src/assets/css/flexslider.css",
              "src/assets/css/theme-blue.css",
              "src/assets/css/theme-cherry.css",
              "src/assets/css/theme-teal.css",
              "src/assets/css/theme-red.css",
              "src/assets/css/theme-pink.css",
              "src/assets/css/theme-green.css",
              "src/assets/css/theme-orchid.css",
              "src/assets/css/theme-jade.css",
              "src/assets/css/theme-skyblue.css",
              "src/assets/css/theme-orange.css",
              "src/assets/css/theme-yellow.css",
              "src/assets/css/theme-blue2.css"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/jquery-validation/dist/jquery.validate.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pfe:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pfe:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pfe:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/assets/css/base.css",
              "src/assets/css/theme-default.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/animate.min.css",
              "src/assets/css/settings.css",
              "src/assets/css/style.css",
              "src/assets/css/media_queries.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/shotcodes.css",
              "src/assets/css/jquery.fancybox.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/owl.theme.css",
              "src/assets/css/flexslider.css",
              "src/assets/css/theme-blue.css",
              "src/assets/css/theme-cherry.css",
              "src/assets/css/theme-teal.css",
              "src/assets/css/theme-red.css",
              "src/assets/css/theme-pink.css",
              "src/assets/css/theme-green.css",
              "src/assets/css/theme-orchid.css",
              "src/assets/css/theme-jade.css",
              "src/assets/css/theme-skyblue.css",
              "src/assets/css/theme-orange.css",
              "src/assets/css/theme-yellow.css",
              "src/assets/css/theme-blue2.css"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/jquery-validation/dist/jquery.validate.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "pfe:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "pfe:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "pfe"
}